{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "interface-name": [true, "never-prefix"],
        "no-implicit-dependencies": [true, "dev"],
        "no-var-keyword": true,
        "no-parameter-reassignment": true,
        "typedef": [true, "call-signature"],
        "no-if-statement": true
    },
    "rulesDirectory": []
}
